<body>
    <script> 
        let mangSo = [5, 10, 15, 20, 25, 30, 35, 40, 45, 50];

        let soNhap = parseInt(prompt("Nhập vào một số bất kỳ:"));

        let flag = true;
        for (let i = 0; i < mangSo.length; i++) {
            if (mangSo[i] === soNhap) {
                flag = false;
                break;
            }
        }
        if (flag === false) {
            alert("Bingo");
        } else {
            alert("Chúc bạn may mắn lần sau");
        }
    </script>
</body>
</html>
