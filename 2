<body>
    <script> 
        const mangSo = [5, 10, 15, 20, 25, 30, 35, 40, 45, 50];

        const soNhap = parseInt(prompt("Nhập vào một số bất kỳ:"));

        if (mangSo.includes(soNhap)) {
            alert("Bingo");
        } else {
            alert("Chúc bạn may mắn lần sau");
        }
    </script>
