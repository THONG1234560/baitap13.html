    <script> 
        let arr = [];

            while (arr.length < 10) {
            let randomNumber = Math.floor(Math.random() * 100);
        arr.push(randomNumber);
        }

        console.log("Mảng gồm 10 phần tử ngẫu nhiên:", arr);
        alert("Mảng gồm 10 phần tử ngẫu nhiên: " + arr.join(", "));

    </script>
